						if (!prevLoc) {
							prevLoc = ui.originalPosition;
						}

						var offsetLeft = currentLoc.left-prevLoc.left;
						var offsetTop = currentLoc.top-prevLoc.top;

						moveSelectedgraph(offsetLeft, offsetTop);
						$(this).data('prevLoc', currentLoc);
					}
				};

